case ${TERM} in
    xterm*|rxvt*|Eterm|aterm|kterm|gnome*)
        export TERM="xterm-256color"
        if [ $(tput colors) -ge "256" ]; then
            _theme-powerline
            _powerline-nerd
        else
            _theme-simple
            _simple-nerd
        fi
        ;;
    *)
        _theme-simple
        _simple-nerd
        ;;
esac

export SSO_THEME='auto'
